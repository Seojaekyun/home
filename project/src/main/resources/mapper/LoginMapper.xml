<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.LoginMapper">
	<select id="loginOk" resultType="String">
		select name from member where userid=#{userid} and pwd=#{pwd}
	</select>
	<select id="loginAdmin" resultType="String">
		select name from admin where userid=#{userid} and pwd=#{pwd}
	</select>
	<select id="getMemberByUserId" parameterType="String" resultType="com.example.demo.dto.MemberDto">
		SELECT * FROM member WHERE userid = #{userid}
	</select>
	
	
</mapper>